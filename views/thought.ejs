            <!-- List started here -->

            <% var month=["JAN","FEB","MAR","APR","MAY","JUN","JULY","AUG","SEPT","OCT","NOV","DEC"] %>
                    <div class="main-timeline maintemp" style="width: 100%; margin-top: 70px;">
                    <% for(var i=thought.length-1;i>=0;i--){ %>
                        <!-- start experience section-->
                        <div class="timeline">
                            <div class="icon"></div>
                            <div class="date-content">
                                <div class="date-outer">
                                    <span class="date">
                                        <span class="month"><%= month[thought[i].date.getMonth()] %>&nbsp;<%= thought[i].date.getDate() %></span>
                                        <span class="year"><%= thought[i].date.getFullYear()%></span>
                                        <span class="month"><%= thought[i].date.getHours() + " : " + thought[i].date.getMinutes()  %></span>
                                        
                                    </span>
                                </div>
                            </div>
                            <div class="timeline-content">
                                <h5 class="title"><%= thought[i].studentName %></h5>
                                <p class="description">
                                    <%= thought[i].thought %>
                                </p>
                                
                            </div>
                        </div>
                        <!-- end experience section-->

                        <%}%>

                        
                

                        
                        <div id="mybutton">
                            <a href="/managethought"><button  class="feedback">My Thought</button></a>
                            </div>

                    </div>
                
            




