                        <div class="row">
                            
                                <!-- USER DATA-->
                                
                                    
                                <form action="/managethought" method="POST"> 
                                  
                                    
                                    
                                    <div class="form-group">
                                        <label for="exampleFormControlTextarea1">Example textarea</label>
                                        <textarea class="form-control" id="exampleFormControlTextarea1" rows="5" name="thought"></textarea>
                                      </div>
                                    <button type="submit" class="btn btn-primary" style=" margin-left: 50%; width: 100px; height: 40px; border-radius: 10px;">Submit</button>
                                  </form>
                                    
                                
                                <!-- END USER DATA-->
                           
                            
                        </div>


                        <% var col=["bg-primary","bg-secondary","bg-success","bg-info","bg-warning","bg-danger"] %>
                        <div class="row">
                            <h3 class="title-3 m-b-30">
                                <i class="zmdi zmdi-account-calendar"></i>MyThought</h3>
                            <div class="container">
                                <div class="row mb-5">

                                    <% for(var i=0;i<thought.length;i++){ %>
                                    
                                   <div class="col-md-4 mb-5">
                                      <div class="card <%= col[(i+1)%6] %> mb-1 text-center">
                                         <div class="card-body">
                                            <blockquote class="blockquote">
                                               <p><%= thought[i].thought %></p>
                                               
                                            </blockquote>
                                            <span>Date : &nbsp;<%= thought[i].date.toString().substring(0,15) %></span>
                                            <form action="/deletethought" method="POST">
                                            <button style="background-color: #fff; color: #eb2727; width: 80px; height: 40px; border-radius: 15px; margin-top: 5px;" type="submit" name="id" value="<%= thought[i]._id %>">Delete</button>
                                            </form>
                                         </div>
                                      </div>
                                   </div>
                                   
                                   <%}%>
                                   
                                </div>
                             </div>
                        </div>


                       
                         
                          

			
                    
                   

					

