<DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <!-- ===== BOX ICONS ===== -->
        <link href='https://cdn.jsdelivr.net/npm/boxicons@2.0.5/css/boxicons.min.css' rel='stylesheet'>

        <!-- ===== CSS ===== -->
        <link rel="stylesheet" href="CSS/dashboard.css">
        <link href="CSS/theme.css" rel="stylesheet" media="all">
    
        

        <title>DSCSDEC</title>
    </head>
    <body id="body-pd">
        <header class="header" id="header">
            <div class="header__toggle">
                <i class='bx bx-menu' id="header-toggle"></i>
                <span>Academics</span>
            </div>

            
           
        </header>

        <div class="l-navbar" id="nav-bar">
            <nav class="nav">
                <div>
                    <a href="/" class="nav__logo" title="Dr. Sudhir Chandra Sur Degree Engineering College">
                        <img src="images/logo.jpeg" class="main-logo">
                        <span class="nav__logo-name">DSCSDEC</span>
                    </a>
    
                    <div class="nav__list">
                        <a href="/dashboard" class="nav__link " title="Dashboard" type="submit">
                            <i class='bx bx-home nav__icon'></i>
                            <span class="nav__name">Dashboard</span>
                        </a>
    
                        <a href="/resource" class="nav__link" title="Resources">
                            <i class='bx bx-book nav__icon'></i>
                            <span class="nav__name">Resources</span>
                        </a>
    
                        <a href="/event" class="nav__link" title="Event">
                            <i class='bx bx-notification nav__icon'></i>
                            <span class="nav__name">Event</span>
                        </a>
                        <a href="/notice" class="nav__link" title="Notice">
                            <i class='bx bxs-news nav__icon'></i>
                            <span class="nav__name">Notice</span>
                        </a>
    
                        <a href="/acedmics" class="nav__link active" title="Acedmics">
                            <i class='bx bx-book-reader nav__icon'></i>
                            <span class="nav__name">Acedmics</span>
                        </a>
    
                        <a href="/articles" class="nav__link" title="Articles">
                            <i class='bx bx-book-content nav__icon'></i>
                            <span class="nav__name">Articles</span>
                        </a>
    
                        <a href="/thought" class="nav__link" title="Thought Wall">
                            <i class='bx bx-bulb nav__icon'></i>
                            <span class="nav__name">Thought Wall</span>
                        </a>
                    </div>
                </div>
    
                <a href="/users/logout" class="nav__link" title="Log Out">
                    <i class='bx bx-log-out nav__icon'></i>
                    <span class="nav__name">Log Out</span>
                </a>
            </nav>
        </div>
        

       <div class="main-frame">
        <div class="main">


            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">

                        


                        
                        <div class="row">
                            <div class="col-lg-6">
                                <!-- USER DATA-->
                                
                                    
                                    <div class="table-responsive table-data">
                                        <table class="table table-borderless table-striped table-earning">
                                            <thead>
                                                <h1>Subjects</h1>
                                                <tr>
                                                    <th>Subject</th>
                                                    <th>Code</th>
                                                    <th>Type</th>
                                                   
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for(var i=0;i<subject.length;i++){ %>
                                                <tr>
                                                    <td><%= subject[i].subjectName %></td>
                                                    <td><%= subject[i].subjectId %></td>
                                                    <td><%= subject[i].subjectType %></td>
                                                </tr>
                                                <%}%>
                                                
                                            
                                                
                                            </tbody>
                                        </table>
                                    </div>
                                    
                                
                                <!-- END USER DATA-->
                            </div>
                            <div class="col-lg-6">
                                <!-- TOP CAMPAIGN-->
                                <div class="user-data m-b-30">
                                    <h3 class="title-3 m-b-30">
                                        <i class="zmdi zmdi-account-calendar"></i>Top Performer</h3>
                                    
                                    <div class="table-responsive table-data">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <td>
                                                       Student ID
                                                    </td>
                                                    <td>Test</td>
                                                    <td>Date</td>
                                                    <td>Obtain Marks</td>
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for(var i=0;i<marks.length;i++){ %>  
                                                <tr>
                                                    <td>
                                                        <%= topId[i] %>
                                                    </td>
                                                    <td>
                                                       <%= examname[i]  %> 
                                                    </td>
                                                    <td>
                                                        <%= examdate[i]  %> 
                                                    </td>
                                                   
                                                    <td>
                                                        <%= topmarks[i]  %>  <span style="font-size: 40px; ">/</span><%= marks[i].fullMarks  %> 
                                                    </td>
                                                    
                                                </tr>
                                                <%}%>
                                                
                                            </tbody>
                                        </table>
                                    </div>
                                    
                                </div>
                                <!--  END TOP CAMPAIGN-->
                            </div>
                        </div>



                        <div class="row">
                            <div class="col-lg-6">
                                <!-- USER DATA-->
                                <div class="user-data m-b-30">
                                    <h3 class="title-3 m-b-30">
                                        <i class="zmdi zmdi-account-calendar"></i> Your Test Result</h3>
                                    
                                    <div class="table-responsive table-data">
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <td>
                                                       Test Name
                                                    </td>
                                                    <td>Subject Id</td>
                                                    <td>Date</td>
                                                    <td>Marks</td>
                                                    
                                                </tr>
                                            </thead>
                                            <tbody>

                                                <% for(var i=0;i<marks.length;i++){ %> 
                                                <tr>
                                                    <td>
                                                        <%= examname[i]  %>
                                                    </td>
                                                    <td>
                                                        <%= subjectCode[i]  %>
                                                    </td>
                                                    <td>
                                                        <%= examdate[i]  %>
                                                    </td>
                                                    <td>
                                                        <%= usermarks[i]  %> <span style="font-size: 30px;">/</span><%= marks[i].fullMarks  %>
                                                    </td>
                                                </tr>
                                                <%}%>
                                                
                                            </tbody>
                                        </table>
                                    </div>
                                    
                                </div>
                                <!-- END USER DATA-->
                            </div>
                            <div class="col-lg-6">
                                <!-- TOP CAMPAIGN-->
                                <div class="top-campaign">
                                    <h3 class="title-3 m-b-30">Upcoming Class Test</h3>
                                    <div class="table-responsive">
                                        <table class="table table-top-campaign">
                                            <tbody>
                                                <tr>
                                                    <td>Exam</td>
                                                    <td>Date</td>
                                                </tr>
                                                <% for(var i=0;i<exam.length;i++){ if(!examID.includes(exam[i].examId)){ %> 
                                                <tr>
                                                    <td><%= exam[i].examTitle %></td>
                                                    <td><%= exam[i].examDate.toString().substring(0,15) %></td>
                                                </tr>
                                                <%}}%>
                                                
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <!--  END TOP CAMPAIGN-->
                            </div>
                        </div>


                       
                         
                          

			
                    
                   

					

				</div>
			</main>

			<footer class="footer" style="background-color: rgb(105, 138, 127); color: #fff;">
				<div class="container-fluid">
					<div class="row text-muted">
						<div class="col-6 text-left" >
							<p class="mb-0" style="color: #fff;" >
								<strong>Rohit Shaw</strong> &copy;
							</p>
						</div>
